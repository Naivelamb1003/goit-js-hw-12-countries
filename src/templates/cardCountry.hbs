
<div class="country__item">
    <h1>{{name}} </h1>
    <h2> Capital:{{capital}}</h2>
    <h2> Population:{{population}}</h2>
    <h2> Languages:
        <ul>{{#each languages}}
            <li>{{name}}</li>
            {{/each}}</ul>
      <img
      src={{flag}}
      class="flag__image"
    />
    </div>
